{
  "name": "gatsby-client",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-client",
  "author": "Wouter",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "forestry:preview": "gatsby develop -p 8080 -H 0.0.0.0",
    "start:dev": "docker compose -f ../docker/gatsby/docker-compose-dev.yml up --remove-orphans -d",
    "stop:dev": "docker compose -f ../docker/gatsby/docker-compose-dev.yml stop",
    "build:dev": "docker compose -f ../docker/gatsby/docker-compose-dev.yml build",
    "clean:dev": "docker compose -f ../docker/gatsby/docker-compose-dev.yml down --rmi local --volumes --remove-orphans"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@reach/router": "^1.3.4",
    "axios": "^0.24.0",
    "babel-plugin-styled-components": "^1.13.3",
    "bootstrap": "^5.1.3",
    "framer-motion": "^5.4.5",
    "gatsby": "^4.1.3",
    "gatsby-plugin-image": "^2.1.2",
    "gatsby-plugin-mdx": "^3.1.3",
    "gatsby-plugin-netlify-cms": "^6.1.0",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-sharp": "^4.1.3",
    "gatsby-plugin-sitemap": "^5.1.1",
    "gatsby-plugin-styled-components": "^5.1.0",
    "gatsby-source-filesystem": "^4.1.2",
    "gatsby-transformer-json": "^4.4.0",
    "gatsby-transformer-sharp": "^4.1.0",
    "netlify-cms-app": "^2.15.59",
    "react": "^17.0.1",
    "react-bootstrap": "^2.0.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-slick": "^0.28.1",
    "styled-components": "^5.3.3"
  }
}
